<ServerManagerConfiguration>
  <ProxyGroup name="filters">


   <SourceProxy name="FlowTopo" class="vtkFlowTopo">
    <InputProperty
       name="Input"
       command="SetInputConnection">
          <ProxyGroupDomain name="groups">
            <Group name="sources"/>
            <Group name="filters"/>
          </ProxyGroupDomain>
          <DataTypeDomain name="input_type">
            <DataType value="vtkDataSet"/>
          </DataTypeDomain>
          <InputArrayDomain name="input_array1" attribute_type="point" number_of_components="3">
	    <RequiredProperties>
              <Property name="SelectInputVectors" function="FieldDataSelection"/>
            </RequiredProperties>
	  </InputArrayDomain>
     </InputProperty>

     <StringVectorProperty 
        name="SelectInputVectors" 
        command="SetInputArrayToProcess"
        number_of_elements="5"
        element_types="0 0 0 0 2">
           <ArrayListDomain name="array_list" attribute_type="Vectors" input_domain_name="input_array1">
             <RequiredProperties>
                <Property name="Input" function="Input"/>
             </RequiredProperties>
           </ArrayListDomain>
           <FieldDataDomain name="field_list">
             <RequiredProperties>
                <Property name="Input" function="Input"/>
             </RequiredProperties>
           </FieldDataDomain>
     </StringVectorProperty> 

     <IntVectorProperty 
        name="DivideByWallDist" 
        command="SetDivideByWallDist" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain name="bool"/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="InteriorCritPts" 
        command="SetInteriorCritPts" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain name="bool"/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="BoundaryCritPts" 
        command="SetBoundaryCritPts" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain name="bool"/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="GenerateSeeds" 
        command="SetGenerateSeeds" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain name="bool"/>
     </IntVectorProperty>

     <IntVectorProperty
        name="SeedsPerCircle"
        command="SetSeedsPerCircle"
        number_of_elements="1"
        default_values="8">
       <IntRangeDomain name="range"/>
     </IntVectorProperty>

     <DoubleVectorProperty
        name="Radius"
        command="SetRadius"
        number_of_elements="1"
        default_values="1.0">
       <DoubleRangeDomain name="range" min="0" max="1000000"/>
     </DoubleVectorProperty>

     <DoubleVectorProperty
        name="Offset"
        command="SetOffset"
        number_of_elements="1"
        default_values="1.0">
       <DoubleRangeDomain name="range" min="0" max="1000000"/>
     </DoubleVectorProperty>

     <DoubleVectorProperty
        name="GlyphRadius"
        command="SetGlyphRadius"
        number_of_elements="1"
        default_values="0.1">
       <DoubleRangeDomain name="range" min="0" max="1000000"/>
     </DoubleVectorProperty>

   <!-- End FlowTopo -->
   </SourceProxy>


  </ProxyGroup>
</ServerManagerConfiguration>
